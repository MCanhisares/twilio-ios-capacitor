{
  "name": "twilio-ios-capacitor",
  "version": "0.1.0",
  "description": "This plugin is used to make video calls using Twilio video platform on iOS",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "MCanhisares",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "latest"
  },
  "devDependencies": {
    "typescript": "^2.6.2"
  },
  "files": [
    "dist/",
    "ios/",    
    "TwilioIosCapacitor.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MCanhisares/twilio-ios-capacitor"
  },
  "bugs": {
    "url": "https://github.com/MCanhisares/twilio-ios-capacitor/issues"
  }
}
